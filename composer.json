{
    "name": "ingmarkleine/pdfjs-viewer-bundle",
    "type": "symfony-bundle",
    "keywords": ["pdfjs", "Symfony", "Symfony4", "pdf viewer"],
    "description": "This bundle provides an integration of pdf.js from mozilla with Symfony 4 framework and customizable parameters on server side.",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "symfony/symfony": "^4.3@dev",
        "symfony/flex": "^1.2@dev"
    },
    "autoload": {
        "psr-0": { "PDFjsViewerBundle" : "" }
    },
    "target-dir" : "PDFjsViewerBundle",
    "minimum-stability": "dev",
    "extra" : {
        "branch-alias" : {
            "dev-master" : "4.0-dev"
        }
    }
}
